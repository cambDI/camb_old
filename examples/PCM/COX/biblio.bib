number = {20},
journal = {Bioorganic \& medicinal chemistry},
author = {Prusis, Peteris and Lapins, Maris and Yahorava, Sviatlana and Petrovska, Ramona and Niyomrattanakit, Pornwaratt and Katzenmeier, Gerd and Wikberg, Jarl E S},
