pages = {357–364},
file = {Full Text PDF:/Bis/home/icortes/.zotero/zotero/46f1slvv.default/zotero/storage/XMM369CT/Bieler and Koeppen - 2012 - The Role of Chemogenomics in the Pharmaceutical In.pdf:application/pdf;Snapshot:/Bis/home/icortes/.zotero/zotero/46f1slvv.default/zotero/storage/AUZ7AX6M/abstract.html:text/html}
